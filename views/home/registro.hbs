<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Registro</title>
   <link rel="stylesheet" href="assets/css/Login.css">
  </head>
  <body>
    <div class="login-box">
      <div>
        <img src="assets/images/logos/logoblanco.png" alt="">
      </div>
      <form id="form">
        <div class="user-box">
          <input type="text" name="nombre" id="nombre">
          <label>Nombre completo</label>
        </div>
        <div class="user-box">
            <input type="text" name="correo" id="correo">
            <label>Correo</label>
          </div>
          <div class="user-box">
            <input type="password" name="contrasena" id="contrasena">
            <label>Contraseña</label>
          </div>
        <div class="user-box">
          <input type="password" name="confirmarContrasena" id="confirmarContrasena">
          <label>Confirmar contraseña</label>
        </div>
        <center><div><br>
          <button id="botonRegistrar" class="btn-neon" type="submit">
          <span id="span1"></span>
          <span id="span2"></span>
          <span id="span3"></span>
          <span id="span4"></span> Registrarse </button>
          <p class="warnings" id="warnings" ></p>
        </div></center>
      </form>
    </div>
    </div>
  <script src="assets/js/validarRegistro.js"></script>
  </body>
</html>
